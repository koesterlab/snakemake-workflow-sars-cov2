    "import pandas as pd\n",
    "import altair as alt\n",
    "\n",
    "calls = pd.read_csv(snakemake.input[0])\n",
    "calls = calls.loc[calls[\"status\"] != \"fail\"]\n",
    "\n",
    "alt.Chart(calls).mark_bar().encode(\n",
    "    y=alt.Y(\"lineage:N\", title=\"\"),\n",
    "    x=\"probability\"\n",
    ").save(snakemake.output[0])"
   ]
  }
